def top_row():
	print '+','-','-','-','-','+','-','-','-','-','+'
def mid_row():
	print '|',' ',' ',' ',' ','|',' ',' ',' ',' ','|'

top_row()
mid_row()
mid_row()
mid_row()
mid_row()
top_row()
mid_row()
mid_row()
mid_row()
mid_row()
top_row()


